{
  "required_api_version": "^2.0.0",
  "name": "AnimeGo",
  "description": "Search AnimeGo directly within ULauncher",
  "developer_name": "Lukser",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.5
  },
  "preferences": [
    {
      "id": "ag_kw",
      "type": "keyword",
      "name": "AnimeGo",
      "description": "Open keyword",
      "default_value": "ag"
    },
    {
      "id": "title_format",
      "type": "select",
      "name": "Title format",
      "description": "Your preferred title format",
      "default_value": "Russian",
      "options": ["Romaji", "Russian"]
    },
    {
      "id": "search_mode",
      "type": "select",
      "name": "Search Mode",
      "description": "Type of content to search for",
      "default_value": "Anime",
      "options": ["All", "Anime", "Manga"]
    }
  ]
}
